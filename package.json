{
  "name": "learning-outcome-suggestion",
  "version": "1.0.0",
  "description": "Semantic search to recommend related learning outcomes in Bloomin Onion",
  "main": "src/lo-suggestion.service.ts",
  "scripts": {
    "build": "tsc -p ts/",
    "build:watch": "tsc -p ts/ -w",
    "setup": "node script/db-setup.script.js",
    "prestart": "mongod -f mongod.conf",
    "start": "npm run build:watch",
    "stop": "mongod -f mongod.conf --shutdown",
