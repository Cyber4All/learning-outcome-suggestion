  "dependencies": {
    "express": "^4.16.1",
    "jsonfile": "^4.0.0",
    "line-reader": "^0.4.0",
    "mongodb": "^2.2.31",
    "mysql": "^2.15.0",
    "request": "^2.83.0",
    "socket.io": "^2.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.0.37",
    "@types/jsonfile": "^4.0.0",
    "@types/line-reader": "0.0.28",
